<ul>
  <li><strong><%= @restaurant.name %></strong></li>
  <li><%= @restaurant.address %></li>
  <li><%= @restaurant.phone_number %></li>
  <li><%= @restaurant.category %></li>
</ul>

<%= link_to "Go back to all restaurants", restaurants_path %>

<ul>
  <% @restaurant.reviews.each do |review| %>
    <li>
      <%= review.content %>---- <%= review.rating * '*'%></li>
  <% end %>
</ul>


<%= link_to "Add a review", new_restaurant_review_path(@restaurant) %>
